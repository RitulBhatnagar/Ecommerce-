{
  "name": "Ecommerce",
  "version": "1.0.0",
  "description": "backend system for Ecommerce apis",
  "main": "index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "start:worker": "npx ts-node src/workers/emailWorker.ts",
    "start:api": "ts-node src/index.ts",
    "start:dev": "concurrently \"npm run start:api\" \"npm run start:worker\""
  },
  "author": "Ritul Bhatnagar",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "argon2": "^0.40.3",
    "bullmq": "^5.12.8",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.14",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.3.0",
    "@types/nodemailer": "^6.4.15",
    "@types/supertest": "^6.0.2",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "prisma": "^5.18.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
